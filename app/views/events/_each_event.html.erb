<div class="row">
  <div class="event-div">
    <div class="event-title">
      <h4>
        <%= link_to event.title, event_path(event) %>
      </h4>
      <p>
        <%= occurrence.humanized_start_time %>
        @ <%= event.venue.name %>&emsp;
        <%= what_cost(event.cost) %>
      </p>
    </div>
    <div class="event-details">
      <p>
        <%= event.venue.address %>
      </p>
      <p>
        From <%= occurrence.start_time.strftime("%l:%M %P") %> to <%= occurrence.end_time.strftime("%l:%M %P") %>
      </p>
      <p>
        <%= event_details_format(event.details) %>
      </p>
    </div>
  </div>
</div>