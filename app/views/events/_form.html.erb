<%= simple_form_for(@event) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :title %>
    <%= f.input :venue %>
    <%= f.input :address %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :date %>
    <%= f.input :cost %>
    <%= f.input :details %>
    <%= f.input :fav %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>