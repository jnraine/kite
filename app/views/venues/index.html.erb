<div class="row col-md-12 form-header">
	<h3>Manage your venues</h3>
	<p><%= link_to 'Add New Venue', new_venue_path %></p>
</div>

<table class="table table-hover">
<% @venues.each do |venue| %>
<% edit = edit_venue_path(venue) %>
  <tr>
    <td><%= link_to venue.name, edit %></td>
    <td><%= link_to venue.address, edit %></td>
    <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>