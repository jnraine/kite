<%= simple_form_for(@venue, defaults: { input_html: { class: "form-control" }}) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name %>
  <%= f.input :address %>

  <%= f.input :submit, :wrapper => :bs_submit_button do %>
    <%= f.submit "Add venue", :class => "btn btn-primary" %>
  <% end %>
<% end %>